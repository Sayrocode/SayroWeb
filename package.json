{
  "name": "real-estate-portal",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true next build",
    "ego:export": "node --loader ts-node/esm src/cli/ego-export.ts",
    "ego:scrape": "node --loader ts-node/esm src/ego/scrapeToDb.ts",
    "prisma:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:seed:news": "node prisma/seed-news.mjs",
    "postinstall": "prisma generate",
    "db:sql": "prisma migrate diff --from-empty --to-schema-datamodel prisma/schema.prisma --script > prisma/schema.sql",
    "db:deploy:turso": "node prisma/deploy-to-turso.mjs",
    "turso:apply": "node prisma/deploy-to-turso.mjs",
    "turso:setup": "yarn turso:apply && yarn db:seed:news",
    "db:create-admin": "node prisma/create-admin.mjs"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@libsql/client": "^0.8.1",
    "@prisma/adapter-libsql": "^5.22.0",
    "@prisma/client": "^5.20.0",
    "@tanstack/query-sync-storage-persister": "^5.90.13",
    "@tanstack/react-query": "^5.90.11",
    "@tanstack/react-query-persist-client": "^5.90.13",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "facebook-nodejs-business-sdk": "^17.0.0",
    "framer-motion": "^10.16.0",
    "iron-session": "^8.0.2",
    "leaflet": "^1.9.4",
    "next": "^16.0.0",
    "playwright": "^1.46.1",
    "postcss-import": "^16.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "^3.2.5",
    "swr": "^2.2.0",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "eslint": "8.50.0",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "seed": "node prisma/seed.mjs"
  }
}
